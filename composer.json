{
    "name": "sirgrimorum/pages",
    "type": "library",
    "description": "Content manager by pages for Laravel 5.6",
    "keywords": [
        "sirgrimorum",
        "Pages",
        "CMS"
    ],
    "homepage": "https://github.com/sirgrimorum/pages",
    "license": "MIT",
    "authors": [
        {
            "name": "SirGrimorum",
            "email": "andres.espinosa@grimorum.com",
            "homepage": "https://github.com/sirgrimorum",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.6",
        "sirgrimorum/transarticles" : ">1.2",
        "sirgrimorum/crudgenerator" : ">3.7.6"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "Sirgrimorum\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.6-dev"
        },
        "laravel": {
            "providers": [
                "Sirgrimorum\\Pages\\PagesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
